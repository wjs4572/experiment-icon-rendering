{
  "name": "icon-performance-testing",
  "version": "1.0.0",
  "description": "Academic research tool for icon rendering performance analysis",
  "private": true,
  "scripts": {
    "test": "playwright test --reporter=html,line",
    "test:full": "playwright test --reporter=html,line",
    "test:commit": "node scripts/commit-tests.js",
    "test:commit:dry": "node scripts/commit-tests.js --dry",
    "test:rotation": "node scripts/commit-tests.js --rotation",
    "test:status": "node scripts/commit-tests.js --status",
    "test:quick": "playwright test tests/index.test.js tests/summary.test.js --reporter=html,line",
    "test:browser:chrome": "playwright test --project=chromium --reporter=html,line",
    "test:browser:firefox": "playwright test --project=firefox --reporter=html,line",
    "test:browser:webkit": "playwright test --project=webkit --reporter=html,line",
    "test:format:json": "playwright test tests/json.test.js tests/geojson.test.js --reporter=html,line",
    "test:format:css": "playwright test tests/css.test.js tests/js.test.js --reporter=html,line",
    "test:format:images": "playwright test tests/png.test.js tests/jpg.test.js tests/gif.test.js --reporter=html,line",
    "test:format:vector": "playwright test tests/svg.test.js tests/avif.test.js tests/webp.test.js --reporter=html,line",
    "test:headed": "playwright test --headed --reporter=line",
    "test:ui": "playwright test --ui",
    "test:report": "playwright show-report",
    "test:install": "playwright install",
    "serve": "npx http-server src -p 3000 --cors"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@types/node": "^20.0.0",
    "http-server": "^14.1.1"
  },
  "keywords": [
    "academic-research",
    "icon-performance",
    "playwright-testing"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/wjs4572/experiment-icon-rendering.git"
  },
  "license": "BSD-3-Clause",
  "main": "go.js",
  "directories": {
    "test": "tests"
  },
  "dependencies": {
    "ansi-styles": "^4.3.0",
    "async": "^3.2.6",
    "basic-auth": "^2.0.1",
    "call-bind-apply-helpers": "^1.0.2",
    "call-bound": "^1.0.4",
    "chalk": "^4.1.2",
    "color-convert": "^2.0.1",
    "color-name": "^1.1.4",
    "corser": "^2.0.1",
    "debug": "^4.4.3",
    "dunder-proto": "^1.0.1",
    "es-define-property": "^1.0.1",
    "es-errors": "^1.3.0",
    "es-object-atoms": "^1.1.1",
    "eventemitter3": "^4.0.7",
    "follow-redirects": "^1.15.11",
    "function-bind": "^1.1.2",
    "get-intrinsic": "^1.3.0",
    "get-proto": "^1.0.1",
    "gopd": "^1.2.0",
    "has-flag": "^4.0.0",
    "has-symbols": "^1.1.0",
    "hasown": "^2.0.2",
    "he": "^1.2.0",
    "html-encoding-sniffer": "^3.0.0",
    "http-proxy": "^1.18.1",
    "iconv-lite": "^0.6.3",
    "math-intrinsics": "^1.1.0",
    "mime": "^1.6.0",
    "minimist": "^1.2.8",
    "ms": "^2.1.3",
    "object-inspect": "^1.13.4",
    "opener": "^1.5.2",
    "playwright": "^1.58.2",
    "playwright-core": "^1.58.2",
    "portfinder": "^1.0.38",
    "qs": "^6.14.2",
    "requires-port": "^1.0.0",
    "safe-buffer": "^5.1.2",
    "safer-buffer": "^2.1.2",
    "secure-compare": "^3.0.1",
    "sharp": "^0.34.5",
    "side-channel": "^1.1.0",
    "side-channel-list": "^1.0.0",
    "side-channel-map": "^1.0.1",
    "side-channel-weakmap": "^1.0.2",
    "supports-color": "^7.2.0",
    "undici-types": "^6.21.0",
    "union": "^0.5.0",
    "url-join": "^4.0.1",
    "whatwg-encoding": "^2.0.0"
  },
  "author": "",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/wjs4572/experiment-icon-rendering/issues"
  },
  "homepage": "https://github.com/wjs4572/experiment-icon-rendering#readme"
}
